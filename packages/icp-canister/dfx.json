{
  "version": 1,
  "canisters": {
    "dhaniverse_backend": {
      "type": "rust",
      "package": "rust-icp-canister",
      "candid": "dhaniverse_backend.did"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral"
    },
    "ic": {
      "providers": ["https://ic0.app"],
      "type": "persistent"
    }
  }
}