{
    "name": "dhaniverse",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
    "dev": "node dev-all.js",
        "dev:web": "npm --prefix web run dev",
        "dev:game": "vite", 
        "dev:servers": "npm run server",
        "build": "npm run build:game",
        "build:web": "cd web && npm run build",
        "build:game": "tsc && vite build",
        "host": "vite --host",
        "preview": "vite preview",
        "test": "vitest",
        "test:run": "vitest run",
        "test:ui": "vitest --ui",
    "server": "cross-env COMSPEC=C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe concurrently \"npm run server:game\" \"npm run server:ws\"",
        "server:game": "deno run -A --unstable-detect-cjs --env-file=server/game/.env --watch server/game/index.ts",
        "server:ws": "deno run -A --env-file=server/ws/.env --watch server/ws/ws.ts",
        "install:all": "npm install && cd web && npm install",
        "icp:generate": "bash scripts/generate-declarations.sh",
        "icp:build": "cd web && npm run build && cd .. && dfx build --network ic",
        "icp:deploy": "bash scripts/deploy-assets.sh",
        "icp:deploy-safe": "bash scripts/deploy-ic-safe.sh",
        "icp:status": "dfx canister --network ic status landing_page",
        "icp:backend-status": "cd packages/icp-canister && dfx canister --network ic status dhaniverse_backend",
        "icp:health": "cd packages/icp-canister && dfx canister --network ic call dhaniverse_backend health_check",
        "icp:setup": "cd packages/icp-canister && bash scripts/setup.sh"
    },
    "devDependencies": {
        "@changesets/cli": "^2.29.5",
        "@tailwindcss/postcss": "^4.1.11",
        "@testing-library/jest-dom": "^6.6.4",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/jest": "^30.0.0",
        "@types/node": "^24.4.0",
        "@vitejs/plugin-react": "4.7.0",
        "@vitest/ui": "^3.2.4",
        "concurrently": "9.2.0",
    "cross-env": "7.0.3",
        "jsdom": "^26.1.0",
        "typescript": "5.8.3",
        "vite": "^6.2.0",
        "vitest": "^3.2.4"
    },
    "dependencies": {
        "@coinbase/wallet-sdk": "4.3.7",
        "@dfinity/agent": "3.0.2",
        "@dfinity/auth-client": "3.0.2",
        "@google-cloud/local-auth": "3.0.1",
        "@livekit/components-react": "^2.9.14",
        "@types/react": "19.1.8",
        "@types/react-dom": "19.1.6",
        "@vercel/speed-insights": "^1.2.0",
        "@walletconnect/ethereum-provider": "^2.16.1",
        "autoprefixer": "10.4.21",
        "clsx": "^2.1.1",
        "ethers": "6.15.0",
        "google-auth-library": "10.1.0",
        "livekit-client": "^2.15.4",
        "lucide-react": "0.525.0",
        "motion": "^12.23.12",
        "path": "0.12.7",
        "phaser": "^3.88.2",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "react-router-dom": "7.7.0",
        "tailwind-merge": "^3.2.1",
        "tailwindcss": "^3.4.11",
        "tailwindcss-animate": "1.0.7"
    }
}
